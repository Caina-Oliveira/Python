{
  "name": "skynet-agent-js",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --ignore-watch node_modules --transpile-only --no-notify src/index.ts",
    "start": "node dist/index.js",
    "prod": "pm2 start dist/index.js --name talisca",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "gen-cache": "ts-node-dev -r tsconfig-paths/register --inspect --ignore-watch node_modules --transpile-only --no-notify src/cache.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/cron": "^1.7.2",
    "@types/lowdb": "^1.0.9",
    "@types/pouchdb": "^6.4.0",
    "@types/underscore": "^1.10.24",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "aws-sdk": "^2.772.0",
    "axios": "^0.20.0",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "jszip": "^3.5.0",
    "lowdb": "^1.0.0",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.2",
    "retry-axios": "^2.2.1",
    "underscore": "^1.11.0"
  }
